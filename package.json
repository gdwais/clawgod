{
  "name": "clawgod",
  "version": "1.0.0",
  "description": "Multi-Agent OpenClaw Instance Generator",
  "bin": {
    "clawgod": "./bin/clawgod.js"
  },
  "files": [
    "bin/",
    "src/",
    "templates/",
    "examples/"
  ],
  "scripts": {
    "generate": "node bin/clawgod.js init",
    "generate:demo": "node bin/clawgod.js init --profile examples/demo-profile.json --non-interactive --output ./output-demo"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "openclaw",
    "ai-agents",
    "multi-agent",
    "cli"
  ],
  "preferGlobal": true,
  "license": "MIT"
}
